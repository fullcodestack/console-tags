{
  "name": "console-tags",
  "version": "0.9.0",
  "description": "Add (custom) CSS-styled tags to your console log output.",
  "author": "Steven van Cauter <steven@fullcodestack.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">= 10.13"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "prestart": "npm run clean",
    "start": "tsc -p scripts/tsconfig.dev.json",
    "prestart:watch": "npm run clean",
    "start:watch": "tsc -w -p scripts/tsconfig.dev.json",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc -p scripts/tsconfig.prod.json"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/node": "^11.10.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tsutils": "^3.8.0",
    "typescript": "^3.3.3"
  }
}
